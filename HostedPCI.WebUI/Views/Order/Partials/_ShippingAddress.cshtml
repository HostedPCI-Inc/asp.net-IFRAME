@model HostedPCI.WebUI.Models.Entities.ShippingAddressModel

<table class="bordered">
    <tr><td colspan="2" class="title">Shipping address</td></tr>
    <tr><td class="label">FirstName:</td><td>@Model.FirstName</td></tr>
    <tr><td class="label">LastName:</td><td>@Model.LastName</td></tr>
    <tr><td class="label">ZipCode:</td><td>@Model.ZipCode</td></tr>
    <tr><td class="label">Country:</td><td>@Model.Country</td></tr>
    <tr><td class="label">State:</td><td>@Model.State</td></tr>
    <tr><td class="label">City:</td><td>@Model.City</td></tr>
    <tr><td class="label">Address:</td><td>@Model.Address</td></tr>
</table>